{
  "name": "atiga-furniture",
  "version": "1.0.0",
  "description": "Unified furniture store application with Prisma",
  "main": "src/server/server.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node src/server/server.js",
    "server": "nodemon src/server/server.js",
    "client": "cd src/client && npm run react-start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd src/client && npm install && npm run build",
    "vercel-build": "npm run prisma:generate && cd src/client && npm install && npm run build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "setup": "npm install && npm run prisma:generate && npm run prisma:migrate"
  },
  "keywords": ["furniture", "e-commerce", "prisma", "react"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^5.10.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "prisma": "^5.10.2"
  }
}
